           MOVE 0 TO KEY-1.
           MOVE "READ-TEST-2" TO PAR-NAME.
           MOVE "DEBUG READ INV KEY" TO FEATURE.
       READ-TEST-2.
           MOVE "05" TO IND-KEY.
           READ IND-FILE  KEY IS IND-KEY
               INVALID KEY  GO TO READ-TEST-2-CONT.
       READ-TEST-2-DELETE.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           GO TO REWRITE-TEST-1-INIT.
       READ-TEST-2-CONT.
           IF KEY-1 IS EQUAL TO 0
               PERFORM PASS
               MOVE "DEBUG PROC NOT EXECUTED" TO RE-MARK
           ELSE  PERFORM FAIL
               MOVE "DEBUG PROC WAS EXECUTED" TO RE-MARK.
           PERFORM GEN-WRITE.
       REWRITE-TEST-1-INIT.
           MOVE SPACES TO ITEM-1.
           MOVE 0 TO KEY-1.
           MOVE "REWRITE-TEST-1" TO PAR-NAME.
           MOVE "DEBUG REWRITE RECORD" TO FEATURE.
       REWRITE-TEST-1.
           MOVE 2 TO XRECORD-NUMBER (1).
           MOVE 1 TO UPDATE-NUMBER (1).
           MOVE FILE-RECORD-INFO (1) TO IND-REC-2.
           MOVE "04" TO IND-KEY.
           MOVE IND-REC-2 TO CONTENTS-REC.
      *> *****************************************************************
      *>     THE DEBUG-LINE (INSPT) TEST NAMED IN THE OUTPUT REPORT AS   *
      *>     "REWRITE-TEST-1A" SHOULD POINT TO THE EXECUTABLE STATEMENT  *
      *>     WHICH FOLLOWS THIS COMMENT SET AND WHICH READS,             *
      *>     "REWRITE IND-REC-1  INVALID KEY  GO TO ABORT-PGM.".         *
      *> *****************************************************************
           REWRITE IND-REC-1  INVALID KEY  GO TO ABORT-PGM.
           IF KEY-1 IS EQUAL TO 1
               PERFORM PASS
               MOVE "DEBUG PROC WAS EXECUTED" TO RE-MARK
               PERFORM GEN-WRITE
               GO TO REWRITE-TEST-1A
           ELSE  PERFORM FAIL
               MOVE "DEBUG PROC NOT EXECUTED" TO RE-MARK
               PERFORM GEN-WRITE
               PERFORM DELETE-REWRITE-TEST-1-SUBTESTS
               GO TO REWRITE-TEST-2-INIT.
       REWRITE-TEST-1-DELETE.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           PERFORM DELETE-REWRITE-TEST-1-SUBTESTS.
           GO TO REWRITE-TEST-2-INIT.
       DELETE-REWRITE-TEST-1-SUBTESTS.
           MOVE "REWRITE-TEST-1A" TO PAR-NAME.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           MOVE "REWRITE-TEST-1B" TO PAR-NAME.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           MOVE "REWRITE-TEST-1C" TO PAR-NAME.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
       REWRITE-TEST-1A.
           MOVE "REWRITE-TEST-1A" TO PAR-NAME.
           MOVE "DEBUG-LINE; SEE NEXT LINE" TO RE-MARK.
           MOVE "<===  DEBUG-LINE" TO CORRECT-A.
           MOVE LINE-1 TO COMPUTED-A.
