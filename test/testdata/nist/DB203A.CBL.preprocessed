               PERFORM PASS
               MOVE "DEBUG PROC WAS EXECUTED" TO RE-MARK
           ELSE  PERFORM FAIL
               MOVE "DEBUG PROC NOT EXECUTED" TO RE-MARK.
           PERFORM GEN-WRITE.
       DELETE-TEST-1-INIT.
           MOVE SPACES TO ITEM-1.
           MOVE 0 TO KEY-1.
           MOVE "DELETE-TEST-1" TO PAR-NAME.
           MOVE "DEBUG DELETE FILE" TO FEATURE.
       DELETE-TEST-1.
           MOVE "06" TO IND-KEY.
           DELETE IND-FILE INVALID KEY GO TO ABORT-PGM.
           IF KEY-1 IS EQUAL TO 1
               PERFORM PASS
               MOVE "DEBUG PROC WAS EXECUTED" TO RE-MARK
               PERFORM GEN-WRITE
               GO TO DELETE-TEST-1A
           ELSE  PERFORM FAIL
               MOVE "DEBUG PROC NOT EXECUTED" TO RE-MARK
               PERFORM GEN-WRITE
               PERFORM DELETE-DELETE-TEST-1-SUBTEST
               GO TO DELETE-TEST-2-INIT.
       DELETE-TEST-1-DELETE.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           PERFORM DELETE-DELETE-TEST-1-SUBTEST.
           GO TO DELETE-TEST-2-INIT.
       DELETE-DELETE-TEST-1-SUBTEST.
           MOVE "DELETE-TEST-1A" TO PAR-NAME.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
       DELETE-TEST-1A.
           MOVE "DELETE-TEST-1A" TO PAR-NAME.
           IF CONTENTS-1 IS EQUAL TO SPACES
