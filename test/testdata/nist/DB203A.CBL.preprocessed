               PERFORM PASS
           ELSE  PERFORM FAIL
               MOVE "IND-REC-1" TO CORRECT-A
               MOVE NAME-1 TO COMPUTED-A.
           MOVE "DEBUG-NAME" TO RE-MARK.
           PERFORM GEN-WRITE.
       REWRITE-TEST-1C.
           MOVE "DEBUG-CONTENTS" TO RE-MARK.
           MOVE "REWRITE-TEST-1C" TO PAR-NAME.
           IF CONTENTS-1 IS EQUAL TO CONTENTS-REC
               PERFORM PASS
               PERFORM GEN-WRITE
               GO TO REWRITE-TEST-2-INIT
           ELSE  PERFORM FAIL
               MOVE "LINES 1 AND 3 BELOW" TO COMPUTED-A
               MOVE "LINES 2 AND 4 BELOW" TO CORRECT-A
               PERFORM GEN-WRITE.
           MOVE CONTENTS-1-1H TO PRINT-REC.
           PERFORM WRITE-LINE.
           MOVE CONTENTS-REC-1H TO PRINT-REC.
           PERFORM WRITE-LINE.
           MOVE CONTENTS-1-2H TO PRINT-REC.
           PERFORM WRITE-LINE.
           MOVE CONTENTS-REC-2H TO PRINT-REC.
           PERFORM WRITE-LINE.
       REWRITE-TEST-2-INIT.
           MOVE SPACES TO ITEM-1.
           MOVE 0 TO KEY-1.
           MOVE "REWRITE-TEST-2" TO PAR-NAME.
           MOVE "DEBUG REWRITE INVLID" TO FEATURE.
       REWRITE-TEST-2.
           MOVE 6 TO XRECORD-NUMBER (1).
           MOVE 1 TO UPDATE-NUMBER (1).
           MOVE FILE-RECORD-INFO (1) TO IND-REC-2.
           MOVE "03" TO IND-KEY.
           REWRITE IND-REC-1  INVALID KEY  GO TO REWRITE-TEST-2-CONT.
       REWRITE-TEST-2-DELETE.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           GO TO START-TEST-1-INIT.
       REWRITE-TEST-2-CONT.
           IF KEY-1 IS EQUAL TO 1
               PERFORM PASS
               MOVE "DEBUG PROC WAS EXECUTED" TO RE-MARK
           ELSE  PERFORM FAIL
               MOVE "DEBUG PROC NOT EXECUTED" TO RE-MARK.
           PERFORM GEN-WRITE.
       START-TEST-1-INIT.
           MOVE SPACES TO ITEM-1.
           MOVE 0 TO KEY-1.
           MOVE "START-TEST-1" TO PAR-NAME.
           MOVE "DEBUG START FILENAME" TO FEATURE.
       START-TEST-1.
           MOVE "05" TO IND-KEY.
           START IND-FILE  KEY IS GREATER THAN IND-KEY
               INVALID KEY  GO TO ABORT-PGM.
           IF KEY-1 IS EQUAL TO 1
               PERFORM PASS
               MOVE "DEBUG PROC WAS EXECUTED" TO RE-MARK
               PERFORM GEN-WRITE
               GO TO START-TEST-1A
           ELSE  PERFORM FAIL
               MOVE "DEBUG PROC NOT EXECUTED" TO RE-MARK
               PERFORM GEN-WRITE
               PERFORM DELETE-START-TEST-1-SUBTEST
               GO TO START-TEST-2-INIT.
       START-TEST-1-DELETE.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
           PERFORM DELETE-START-TEST-1-SUBTEST.
           GO TO START-TEST-2-INIT.
       DELETE-START-TEST-1-SUBTEST.
           MOVE "START-TEST-1A" TO PAR-NAME.
           PERFORM DE-LETE.
           PERFORM GEN-WRITE.
       START-TEST-1A.
           MOVE "START-TEST-1A" TO PAR-NAME.
           IF CONTENTS-1 IS EQUAL TO SPACES
               PERFORM PASS
           ELSE  PERFORM FAIL
               MOVE CONTENTS-1 TO COMPUTED-A
