                   PERFORM PRINT-DETAIL.
           ADD     1 TO REC-CT.
       SEARCH-TEST-2-2.
           MOVE   "SEARCH-TEST-2-2" TO PAR-NAME.
           IF      L3-HOLD = SPACE
                   PERFORM PASS
                   PERFORM PRINT-DETAIL
           ELSE
                   MOVE   "'WHEN' PHRASE SHOULD BE FALSE" TO RE-MARK
                   MOVE    SPACE TO CORRECT-X
                   MOVE    L3-HOLD TO COMPUTED-X
                   PERFORM FAIL
                   PERFORM PRINT-DETAIL.
           ADD     1 TO REC-CT.
       SEARCH-TEST-2-3.
           MOVE   "SEARCH-TEST-2-3" TO PAR-NAME.
           IF      L4-HOLD = "CC"
                   PERFORM PASS
                   PERFORM PRINT-DETAIL
           ELSE
                   MOVE   "SCOPE TERMINATOR IGNORED" TO RE-MARK
                   MOVE   "CC"  TO CORRECT-X
                   MOVE    L4-HOLD TO COMPUTED-X
                   PERFORM FAIL
                   PERFORM PRINT-DETAIL.
       
           GO TO END-SEARCH-TEST.
       
       PASS-TH.
           PERFORM PASS.
           PERFORM PRINT-DETAIL.
       FAIL-TH.
           PERFORM FAIL.
           PERFORM  PRINT-DETAIL.
       END-SEARCH-TEST.
           EXIT.
       CCVS-EXIT SECTION.
       CCVS-999999.
           GO TO CLOSE-FILES.
