           PERFORM END-ROUTINE.
           MOVE     "FIGURATIVE CONSTANTS" TO FEATURE.
       FIG-TEST-1.
           MOVE     SUB-GRP-FOR-2N058-A TO SUB-GRP-FOR-2N058-B.
           IF       SUB-SUB-BA EQUAL TO "000000   ABCABC"
                    PERFORM PASS GO TO FIG-WRITE-1.
           GO       TO FIG-FAIL-1.
       FIG-DELETE-1.
           PERFORM  DE-LETE.
           GO       TO FIG-WRITE-1.
       FIG-FAIL-1.
           MOVE     SUB-SUB-BA TO COMPUTED-A.
           MOVE     "000000   ABCABC" TO CORRECT-A.
           PERFORM  FAIL.
       FIG-WRITE-1.
           MOVE     "FIG-TEST-1" TO PAR-NAME.
           PERFORM  PRINT-DETAIL.
       FIG-TEST-2.
           IF       SUB-SUB-BB EQUAL TO "ZZZ   000000"
                    PERFORM PASS GO TO FIG-WRITE-2.
      *>     NOTE     THIS TEST DEPENDS UPON THE RESULT OF FIG-TEST-1.
           GO       TO FIG-FAIL-2.
       FIG-DELETE-2.
           PERFORM  DE-LETE.
           GO       TO FIG-WRITE-2.
       FIG-FAIL-2.
           MOVE     SUB-SUB-BB TO COMPUTED-A.
           MOVE     "ZZZ   000000" TO CORRECT-A.
           PERFORM  FAIL.
       FIG-WRITE-2.
           MOVE "FIG-TEST-2" TO PAR-NAME.
           PERFORM  PRINT-DETAIL.
       FIG-TEST-3.
           IF       ELEM-FOR-2N058-I OF SUB-SUB-BC NOT EQUAL TO QUOTE
                    GO TO FIG-FAIL-3.
           IF ELEM-FOR-2N058-J OF SUB-SUB-BC NOT EQUAL TO QUOTE
                    GO TO FIG-FAIL-3.
           IF ELEM-FOR-2N058-K OF SUB-SUB-BC NOT EQUAL TO HIGH-VALUE
                    GO TO FIG-FAIL-3.
           IF ELEM-FOR-2N058-L OF SUB-SUB-BC NOT EQUAL TO LOW-VALUE
                    GO TO FIG-FAIL-3.
           IF ELEM-FOR-2N058-M OF SUB-SUB-BC NOT EQUAL TO HIGH-VALUE
                    GO TO FIG-FAIL-3.
           IF ELEM-FOR-2N058-N OF SUB-SUB-BC NOT EQUAL TO LOW-VALUE
                    GO TO FIG-FAIL-3.
           PERFORM  PASS.
           GO       TO FIG-WRITE-3.
       FIG-DELETE-3.
           PERFORM  DE-LETE.
           GO       TO FIG-WRITE-3.
       FIG-FAIL-3.
           MOVE     SPACE TO FEATURE.
           MOVE     "6 QUOTES, 3 HIGH-VALUES," TO RE-MARK.
           PERFORM  PRINT-DETAIL.
           MOVE     "3 LOW-VALUES, 3 HIGH-VALUES" TO RE-MARK.
           PERFORM  PRINT-DETAIL.
           MOVE     "FIGURATIVE CONSTANTS" TO FEATURE.
           MOVE     SUB-SUB-BC TO COMPUTED-A.
           MOVE     "SEE REMARKS" TO CORRECT-A.
           MOVE     "3 LOW-VALUES" TO RE-MARK.
           PERFORM  FAIL.
       FIG-WRITE-3.
           MOVE     "FIG-TEST-3" TO PAR-NAME.
           PERFORM  PRINT-DETAIL.
       CCVS-EXIT SECTION.
       CCVS-999999.
           GO TO CLOSE-FILES.
