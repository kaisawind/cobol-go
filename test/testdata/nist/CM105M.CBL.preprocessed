               03  FILLER PIC X(4).
               03  MSG-COUNT PIC XX.
       PROCEDURE    DIVISION.
       SECT-CM105M-0001 SECTION.
       CM105M-INIT.
           OPEN     OUTPUT PRINT-FILE.
           MOVE "CM105M     " TO TEST-ID.
           MOVE     TEST-ID TO ID-AGAIN.
           MOVE    SPACE TO TEST-RESULTS.
           PERFORM HEAD-ROUTINE.
           PERFORM COLUMN-NAMES-ROUTINE.
           MOVE
           XXXXX030
           TO MAIN-QUEUE.
           MOVE MAIN-QUEUE-NAME TO QUEUE-SET.
           ENABLE INPUT CM-INQUE-1 KEY
           XXXXX031.
           PERFORM BUILD-UP-QUEUES VARYING I1 FROM 1 BY 1
               UNTIL I1 IS GREATER THAN 4.
           GO TO BEGIN-TESTS.
       BUILD-UP-QUEUES.
           MOVE NAME-SET (I1) TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF MSG-COUNT IS LESS THAN 10 GO TO BUILD-UP-QUEUES.
       BEGIN-TESTS.
           DISABLE INPUT CM-INQUE-1 KEY
           XXXXX031.
           MOVE PPPP TO  P  PP  PPP.
           MOVE PPSP TO  PPS.
           MOVE PSPP TO  PS  PSP.
       QUEUE-TEST-01.
           MOVE "QUEUE SERIES PPPP" TO FEATURE.
           MOVE PPPP TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPPP"
               PERFORM PASS GO TO QUEUE-TEST-WRITE-01.
           MOVE "PPPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-01.
           MOVE "QUEUE-TEST-01" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-02.
           MOVE "QUEUE SERIES PPPS" TO FEATURE.
           MOVE PPPS TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPPS"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-02.
           MOVE "PPPS" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-02.
           MOVE "QUEUE-TEST-02" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-03.
           MOVE "QUEUE SERIES PPSP" TO FEATURE.
           MOVE PPSP TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPSP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-03.
           MOVE "PPSP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-03.
           MOVE "QUEUE-TEST-03" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-04.
           MOVE "QUEUE SERIES PSPP" TO FEATURE.
           MOVE PSPP TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PSPP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-04.
           MOVE "PSPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-04.
           MOVE "QUEUE-TEST-04" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-05.
           MOVE "QUEUE SERIES P" TO FEATURE.
           MOVE P TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPPP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-05.
           MOVE "PPPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-05.
           MOVE "QUEUE-TEST-05" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-06.
           MOVE "QUEUE SERIES PP" TO FEATURE.
           MOVE PP TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPPP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-06.
           MOVE "PPPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-06.
           MOVE "QUEUE-TEST-06" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-07.
           MOVE "QUEUE SERIES PPP" TO FEATURE.
           MOVE PPP TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPPP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-07.
           MOVE "PPPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-07.
           MOVE "QUEUE-TEST-07" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-08.
           MOVE "QUEUE SERIES PS" TO FEATURE.
           MOVE PS TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PSPP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-08.
           MOVE "PSPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-08.
           MOVE "QUEUE-TEST-08" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-09.
           MOVE "QUEUE SERIES PSP" TO FEATURE.
           MOVE PSP TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PSPP"
               PERFORM PASS   GO TO QUEUE-TEST-WRITE-09.
           MOVE "PSPP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-09.
           MOVE "QUEUE-TEST-09" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       QUEUE-TEST-10.
           MOVE "QUEUE SERIES PPS" TO FEATURE.
           MOVE PPS TO QUEUE-SET.
           PERFORM RECEIVE-A-MSG.
           IF QUEUE-KEY IS EQUAL TO "PPSP"
               PERFORM PASS  GO TO QUEUE-TEST-WRITE-10.
           MOVE "PPSP" TO CORRECT-QUEUE.
           PERFORM FAIL.
       QUEUE-TEST-WRITE-10.
           MOVE "QUEUE-TEST-10" TO PAR-NAME.
           PERFORM PRINT-DETAIL.
       ACCEPT-TEST-01.
           MOVE "ACCEPT GROUP QUEUE" TO FEATURE.
           MOVE PPPP TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PPPP" TO RE-MARK
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PPPP" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PPPS TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PPPS" TO RE-MARK
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PPPS" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PPSP TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PPSP"  TO RE-MARK
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PPSP" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PSPP TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PSPP" TO RE-MARK
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PSPP" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE P TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR P" TO RE-MARK
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR P" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PP TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PP" TO STAT
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PP" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PPP TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PPP" TO STAT
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PPP" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PS TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PS" TO STAT
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PS" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PSP TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PSP" TO STAT
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PSP" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           MOVE PPS TO QUEUE-SET.
           ACCEPT CM-INQUE-1 COUNT.
           IF IN-STAT IS NOT EQUAL TO "00"
               MOVE IN-STAT TO STAT
               MOVE "BAD STATUS FOR PPS" TO STAT
               ELSE
               MOVE MSG-COUNT TO STAT
               MOVE "COUNT FOR PPS" TO RE-MARK.
           PERFORM ACCEPT-WRITE-01.
           GO TO CM105-FINI.
       ACCEPT-WRITE-01.
           MOVE "ACCEPT-TEST-01" TO PAR-NAME.
           MOVE "INFO" TO P-OR-F.
           PERFORM PRINT-DETAIL.
       CM105-FINI.
           PERFORM END-ROUTINE THRU END-ROUTINE-4.
           CLOSE PRINT-FILE.
           STOP RUN.
       RECEIVE-A-MSG.
           MOVE SPACE TO RE-MARK.
           RECEIVE CM-INQUE-1 MESSAGE INTO RE-MARK
               NO DATA MOVE "NOTHING RECEIVED FROM MCS" TO RE-MARK.
       COMMON-SUBROUTINES SECTION.
       PASS.
           MOVE "PASS" TO P-OR-F.
       FAIL.
           MOVE " SEE REMARKS COLUMN " TO COMPUTED-A.
           ADD      1 TO ERROR-COUNTER.
           MOVE "FAIL*" TO P-OR-F.
       PRINT-DETAIL.
           MOVE     TEST-RESULTS TO PRINT-REC.
           PERFORM WRITE-LINE.
           MOVE     SPACE TO P-OR-F.
           MOVE     SPACE TO COMPUTED-A.
           MOVE SPACE TO CORRECT-A.
           MOVE SPACE TO RE-MARK.
       COLUMN-NAMES-ROUTINE.
           MOVE     COLUMNS-LINE-1 TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
           MOVE     COLUMNS-LINE-2 TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
           PERFORM  BLANK-LINE-PRINT.
       END-ROUTINE.
           MOVE     HYPHEN-LINE TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
       PARA-Z.
           PERFORM  BLANK-LINE-PRINT 4 TIMES.
           MOVE     CCVS-E-1 TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
       END-ROUTINE-1.
           PERFORM  BLANK-LINE-PRINT.
           IF       ERROR-COUNTER IS EQUAL TO ZERO
                    GO TO END-ROUTINE-2.
           MOVE     ERROR-COUNTER TO ERROR-TOTAL.
           GO TO    END-ROUTINE-3.
       END-ROUTINE-2.
           MOVE " NO" TO ERROR-TOTAL.
       END-ROUTINE-3.
           MOVE     CCVS-E-2 TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
           IF DELETE-CNT IS EQUAL TO ZERO
               MOVE " NO" TO ERROR-TOTAL  ELSE
           MOVE DELETE-CNT TO ERROR-TOTAL.
           MOVE "TESTS DELETED     " TO ENDER-DESC.
           MOVE CCVS-E-2 TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
       END-ROUTINE-4.
           MOVE CCVS-E-3 TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
       BLANK-LINE-PRINT.
           MOVE     SPACE TO DUMMY-RECORD.
           PERFORM WRITE-LINE.
       WRITE-LINE.
           WRITE DUMMY-RECORD AFTER ADVANCING 1 LINE.
       HEAD-ROUTINE.
           MOVE CCVS-H-1 TO PRINT-REC
           WRITE PRINT-REC
               AFTER ADVANCING PAGE.
           MOVE CCVS-H-2 TO PRINT-REC.
           WRITE PRINT-REC
               AFTER 2 LINES.
           MOVE CCVS-H-3 TO PRINT-REC.
           WRITE PRINT-REC
               AFTER 5 LINES.
           MOVE HYPHEN-LINE TO PRINT-REC.
           PERFORM WRITE-LINE.
      *> END-OF,CM105M