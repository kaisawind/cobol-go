           SELECT PRINT-FILE ASSIGN TO
           XXXXX055.
           SELECT RW-FS1 ASSIGN TO
           XXXXX049.
       DATA DIVISION.
       FILE SECTION.
       FD  PRINT-FILE
           LABEL RECORDS
           XXXXX084
