       ACC-INIT-F1-14.
           MOVE   "VI-71 6.5.4 GR4(A)" TO ANSI-REFERENCE.
           MOVE    SPACES TO ACCEPT-TEST-14-DATA.
           MOVE   "ACC-TEST-F1-14-1" TO PAR-NAME.
           MOVE   "PLEASE PERFORM A VISUAL CHECK TO ENSURE THAT"
                TO RE-MARK.
           PERFORM PRINT-DETAIL.
           MOVE   "A REQUEST FOR FURTHER INPUT IS MADE BY THE"
                TO RE-MARK.
           PERFORM PRINT-DETAIL.
           MOVE   "HARDWARE DEVICE" TO RE-MARK
           PERFORM PRINT-DETAIL.
       ACC-INIT-F1-14-1.
           MOVE   "ACC-TEST-F1-14-1" TO PAR-NAME.
       ACC-TEST-F1-14-1.
           ACCEPT  ACCEPT-TEST-14-DATA FROM ACCEPT-INPUT-DEVICE.
            IF     ACC-14-CHARS-1-10 = "ABCDEFGHIJ"
                   PERFORM PASS
                   GO TO ACC-WRITE-F1-14-1.
           GO TO ACC-FAIL-F1-14-1.
       ACC-DELETE-F1-14-1.
           PERFORM DE-LETE.
           GO TO ACC-WRITE-F1-14-1.
       ACC-FAIL-F1-14-1.
                   MOVE   "ABCDEFGHIJ" TO CORRECT-A
                           MOVE   ACC-14-CHARS-1-10 TO COMPUTED-A
                   PERFORM FAIL.
       ACC-WRITE-F1-14-1.
                   PERFORM PRINT-DETAIL.
      *> 
       ACC-INIT-F1-14-2.
           MOVE   "ACC-TEST-F1-14-2" TO PAR-NAME.
       ACC-TEST-F1-14-2.
           ACCEPT  ACCEPT-TEST-14-DATA FROM ACCEPT-INPUT-DEVICE.
           IF      ACC-14-CHARS-11-15 = "KLMNO"
                   PERFORM PASS
                   GO TO ACC-WRITE-F1-14-2.
           GO TO ACC-FAIL-F1-14-2.
       ACC-DELETE-F1-14-2.
